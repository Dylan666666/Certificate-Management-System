<mapper namespace="com.oym.cms.mapper.UserMapper">
    <select id="queryUserById" resultType="com.oym.cms.entity.User">
        SELECT 
        *
        FROM 
        tb_staff
        WHERE 
        user_id = #{userId}
    </select>
</mapper>