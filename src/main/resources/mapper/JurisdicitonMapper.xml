<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oym.cms.mapper.JurisdictionMapper">
    <select id="queryAllUrls" resultType="String">
        SELECT 
        url
        FROM 
        jurisdiction
        WHERE 
        url_status = 1 
    </select>
</mapper>